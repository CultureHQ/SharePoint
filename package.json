  "scripts": {
    "analyze": "source-map-explorer dist/main.js",
    "build": "webpack --mode production --config webpack.config.babel.js --progress",
    "start": "webpack-dev-server --mode development --config webpack.config.babel.js --hot",
    "lint": "eslint .",
    "test": "jest --coverage"
