  "jest": {
    "moduleFileExtensions": [
      "js",
      "css"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "./test/setup-tests"
  }
