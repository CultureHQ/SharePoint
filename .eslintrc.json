{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-parens": [2, "as-needed"],
    "comma-dangle": 0,
    "import/extensions": 0,
    "quotes": [2, "double"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".css"]
      }
    }
  }
}
