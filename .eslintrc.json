{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": [2, "as-needed"],
    "comma-dangle": 0,
    "import/extensions": 0,
    "quotes": [2, "double"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/no-did-mount-set-state": 0,
    "react/prop-types": 0
  }
}
